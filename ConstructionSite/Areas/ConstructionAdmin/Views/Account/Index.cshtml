@model IEnumerable<UserDTO>
    
<!--main content start-->
<section id="main-content">
    <section class="wrapper">

        <!-- page start-->
        <div class="row">
            <aside class="profile-nav col-lg-3">
                <section class="panel">
                    <div class="user-heading round">
                        <h1>@Html.DisplayNameFor(model=>model.Name)</h1>
                        <p>@Html.DisplayNameFor(model=>model.Email)</p>
                    </div>
                    <ul class="nav nav-pills nav-stacked">
                        <li class="active"><a asp-controller="Account" asp-action="Index"> <i class="icon-user"></i> Profile</a></li>
                        <li><a asp-controller="Account" asp-action="Edit"> <i class="icon-edit"></i> Edit profile</a></li>
                    </ul>
                </section>
            </aside>
            <aside class="profile-info col-lg-9">
                <section class="panel">
                    <div class="panel-body bio-graph-info">
                        <h1>Bio Graph</h1>
                        <div class="row">
                            <div class="bio-row">
                                <p><span>Full Name </span>: @Html.DisplayNameFor(model=>model.Name)</p>
                            </div>
                            <div class="bio-row">
                                <p><span>Email </span>: @Html.DisplayNameFor(model => model.Email)</p>
                            </div>
                        </div>
                    </div>
                </section>
            </aside>
        </div>
        <!-- page end-->

    </section>
</section>
<!--main content end-->

