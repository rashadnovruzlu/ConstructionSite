@model IEnumerable<UserDTO>

<style>
    .user-profile {
        width: 37%;
        float: left;
        margin-left: -20%;
    }
    .user-bio {
        float: right;
        width: 80%;
        margin-right: 3%;
    }
    .common {
        margin-top: 45px !important;
    }
</style>

<!--main content start-->
<section id="main-content" style="padding-left: 2%; margin-top: -1%;">
    <section class="wrapper common">

        <!-- page start-->
        <div class="row">
            <aside class="profile-nav col-lg-3 user-profile">
                <section class="panel">

                    @await Component.InvokeAsync("UserProfile")
                </section>
            </aside>
            <aside class="profile-info col-lg-9 user-bio">
                <section class="panel">
                    <div class="bio-graph-heading" style="background-color: rgb(49, 49, 49); opacity: .8;">
                        <h1>Bio Graph</h1>
                    </div>

                    @await Component.InvokeAsync("AdminComponents")
                </section>
            </aside>
        </div>
        <!-- page end-->
    </section>
</section>
<!--main content end-->