@model IEnumerable<ServiceMenuViewModel>

   
       
        <ul class="sub-menu">
           
            
         @foreach (var item in Model)
         {
            <li class="menu-item">
                <a asp-controller="Services" asp-action="Renovation">@item.Name</a>
                <ul class="sub-menu2">
                    @foreach (var ite in item.SubServices)
                    {


                        <li class="menu-item"><a asp-controller="Services" asp-action="Architecture">@ite.Name</a></li>

                    }
                </ul>
            </li>
         }
        </ul>
